{
  "name": "@jplag/report-viewer",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check && vite build",
    "build-dev": "run-p type-check && vite build --mode dev",
    "build-demo": "run-p type-check && vite build --mode demo",
    "build-dev-demo": "run-p type-check && vite build --mode dev-demo",
    "type-check": "vue-tsc --noEmit --composite false",
    "test:e2e": "playwright test",
    "test:unit": "vitest",
    "preview": "vite preview"
  },
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/vue-fontawesome": "^3.1.1",
    "@jplag/model": "../model",
    "@jplag/parser": "../parser",
    "@jplag/ui-components": "../ui-components",
    "pinia": "^3.0.3",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@tailwindcss/vite": "^4.1.12",
    "@vitejs/plugin-vue": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^4.1.12",
    "vite": "^7.1.3",
    "vue-tsc": "^3.0.6"
  }
}
